
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int      @default(autoincrement()) @id
  email   String   @unique
  username    String?
  phonenumber String? @unique
}

model Coach {
  id        Int      @default(autoincrement()) @id
  name      String?
  route     String?
}

